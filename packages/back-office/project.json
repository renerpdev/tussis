{
  "name": "back-office",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/back-office/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/back-office",
        "main": "packages/back-office/src/main.ts",
        "tsConfig": "packages/back-office/tsconfig.app.json",
        "assets": ["packages/back-office/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "packages/back-office/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "back-office:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "back-office:build:development"
        },
        "production": {
          "buildTarget": "back-office:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/back-office/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/back-office/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
